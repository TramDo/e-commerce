	<jsp:directive.page contentType="text/html; charset=UTF-8"
		pageEncoding="UTF-8" session="true" />
	<jsp:output doctype-root-element="html"
		doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN"
		doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"
		omit-xml-declaration="true" />
	<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>Results</title>
</head>
<body>
<table>
	<tr><th>Id</th><th>Name</th><th>Credits taken</th><th>Credits to graduate</th></tr>
		
               <tr><td>${requestScope['sid'] }</td>
			<td>${requestScope['name'] }</td>
			<td>${requestScope['taken'] }</td>
			<td>${requestScope['grad'] }</td></tr>


	
	</table>
	
	<BR /><BR />
			<button action="restart" name="restart" value="true">Return</button>

</body>
</html>
</jsp:root>